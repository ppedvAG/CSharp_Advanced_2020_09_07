<Window x:Class="Async_Await_Demo.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Async_Await_Demo"
        mc:Ignorable="d"
         Title="Async und Await" Height="172.668" Width="286.777"
        WindowStartupLocation="CenterScreen">
    <StackPanel Margin="0,0,0,-6">
        <TextBlock Margin="5"/>
        <Button x:Name="buttonTask" Content="Start!" Margin="5" Click="Button_Click" />
        <Button Content="Cancel !" Margin="5" Click="Button_Click_1"/>
        <ProgressBar x:Name="progressBarWert" Minimum="0" Maximum="100" Height="30" Margin="5" />
    </StackPanel>
</Window>
